<h1>Listing announcers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Street</th>
    <th>City</th>
    <th>Country</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @announcers.each do |announcer| %>
  <tr>
    <td><%= announcer.name %></td>
    <td><%= announcer.street %></td>
    <td><%= announcer.city %></td>
    <td><%= announcer.country %></td>
    <td><%= announcer.longitude %></td>
    <td><%= announcer.latitude %></td>
    <td><%= link_to 'Show', announcer %></td>
    <td><%= link_to 'Edit', edit_announcer_path(announcer) %></td>
    <td><%= link_to 'Destroy', announcer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Announcer', new_announcer_path %>
